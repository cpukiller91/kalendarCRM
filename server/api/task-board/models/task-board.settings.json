{
  "kind": "collectionType",
  "collectionName": "task_boards",
  "info": {
    "name": "TaskBoard",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "AttachedFiles": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Task": {
      "type": "richtext",
      "required": true
    },
    "comments": {
      "collection": "comments",
      "via": "task_board"
    },
    "endDate": {
      "type": "datetime",
      "required": true
    },
    "Status": {
      "type": "string"
    },
    "Priority": {
      "type": "string"
    },
    "postanovshik": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "project": {
      "via": "task_boards",
      "model": "project"
    },
    "otvetstvenni": {
      "model": "babycard",
      "via": "task_board"
    }
  }
}
